{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/(.*).json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "limits": {
    "bodySize": "10mb"
  }
}
{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/data_chunks/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
